<UserControl x:Class="TestPassModule.Stage.TemplateItems.RelationTemplate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestPassModule.Stage.TemplateItems"
             mc:Ignorable="d"  HorizontalAlignment="Left" VerticalAlignment="Top"
             d:DesignHeight="200" d:DesignWidth="200">
    <Grid x:Name="MainGrid">
        <Rectangle x:Name="CoverBorder"
                   Grid.RowSpan="2"
                   Stroke="Orange"
                   StrokeDashArray="2,2" />

        <Button x:Name="btnSave" Content="Save Relation" Padding="5" HorizontalAlignment="Center" VerticalAlignment="Center"
                Click="btnSave_Click" />
    </Grid>
</UserControl>
