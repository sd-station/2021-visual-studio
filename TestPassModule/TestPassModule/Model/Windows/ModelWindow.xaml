<Window x:Class="TestPassModule.Modules.Windows.ModelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestPassModule.Modules.Windows"
        mc:Ignorable="d" WindowState="Maximized"
        Title="ModuleWindow" Foreground="#cfcfcf"
        Height="450" Background="#212121"
        Width="800">
    <Grid>

        <TextBlock Text="{Binding RelativeSource={RelativeSource Mode=FindAncestor,AncestorType=Window},Path=Title}" 
                   FontSize="48" Margin="10" FontWeight="ExtraLight"
                   Opacity="0.2" Foreground="#cfcfcf" />

        <Frame x:Name="StructFrame" Margin="10,0,0,0"   />

        
        <Frame x:Name="SideFrame" HorizontalAlignment="Right" Width="400" />

    </Grid>
</Window>
