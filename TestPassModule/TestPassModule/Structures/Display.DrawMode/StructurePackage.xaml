<UserControl x:Class="TestPassModule.Structs.Display.StructurePackage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:TestPassModule.Structs.Display"
             mc:Ignorable="d"
             HorizontalAlignment="Left"
             VerticalAlignment="Top"
             d:DesignHeight="700"  
             d:DesignWidth="350">
    
    <Grid Width="300" Height="460" Margin="0,-130,0,0">
        <Rectangle Stroke="#20707070" StrokeThickness="2"
                   StrokeDashArray="5,5" />
        <Canvas x:Name="LineContainer" />
        <Canvas x:Name="PartContainer" />
    </Grid>


</UserControl>
